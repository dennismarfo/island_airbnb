<%= simple_form_for [@property] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :country %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :photo_url %>
  <%= f.input :photo_url_cache, as: :hidden %>
  <%= f.submit "add a property", class: "btn btn-primary" %>
<% end %>
