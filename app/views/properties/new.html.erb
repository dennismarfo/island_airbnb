<%= simple_form_for [@property] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :country, as: "string" %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "add a property", class: "btn btn-primary" %>
<% end %>

